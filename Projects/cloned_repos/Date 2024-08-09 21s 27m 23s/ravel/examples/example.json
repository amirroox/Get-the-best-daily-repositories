{
    "workload": {
        "image": "docker.io/codercom/code-server:latest",
        "registry_auth": {},
        "env": [
            "SUDO_PASSWORD=password",
            "PASSWORD=password"
        ],
        "restart_policy": {
            "policy": "always",
            "max_retries": 5
        },
        "init": {
            "user": "root"
        }
    },
    "guest": {
        "vcpus": 1,
        "cpus": 1,
        "memory_mb": 512
    }
}